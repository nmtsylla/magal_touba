<p id="notice"><%= notice %></p>

<h1>Listing Evenements</h1>

<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th>Descriptif</th>
      <th>Status</th>
      <th>Lieu</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evenements.each do |evenement| %>
      <tr>
        <td><%= evenement.titre %></td>
        <td><%= evenement.descriptif %></td>
        <td><%= evenement.status %></td>
        <td><%= evenement.lieu %></td>
        <td><%= evenement.latitude %></td>
        <td><%= evenement.longitude %></td>
        <td><%= link_to 'Show', evenement %></td>
        <td><%= link_to 'Edit', edit_evenement_path(evenement) %></td>
        <td><%= link_to 'Destroy', evenement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evenement', new_evenement_path %>
